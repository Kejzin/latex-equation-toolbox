{
  "name": "latex-equation-toolbox",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run -s serve || yarn run -s dev",
    "build": "preact build --no-prerender",
    "serve": "preact build --no-prerender && preact serve",
    "deploy": "NODE_ENV=production preact build --no-prerender && gh-pages -d build/",
    "analyze": "ANALYZE=1 preact watch",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest ./src"
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-preact"
    ],
    "rules": {
      "react/jsx-no-bind": false
    }
  },
  "devDependencies": {
    "@types/codemirror": "^0.0.55",
    "@types/jquery": "^3.2.18",
    "@types/lodash": "^4.14.92",
    "@types/mathjax": "^0.0.32",
    "@types/redux": "^3.6.0",
    "@types/webpack": "^3.8.2",
    "awesome-typescript-loader": "3.4.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "exports-loader": "^0.7.0",
    "gh-pages": "^1.1.0",
    "hard-source-webpack-plugin": "^0.7.5",
    "if-env": "^1.0.0",
    "imports-loader": "^0.8.0",
    "preact-cli": "^2.2.1",
    "preact-cli-plugin-typescript": "^0.2.2",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.6.2",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "dependencies": {
    "algebra-latex": "^0.5.1",
    "algebra.js": "^0.2.6",
    "classnames": "^2.2.5",
    "codemirror": "^5.32.0",
    "coffeequate": "^1.3.0",
    "file-loader": "^1.1.5",
    "golden-layout": "^1.5.9",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mathjax": "2.7.2",
    "mathjax-node": "^2.1.1",
    "mathjs": "^4.4.2",
    "mathquill": "^0.10.1-a",
    "node-sass": "^4.5.3",
    "preact": "^8.2.9",
    "react-clipboard.js": "^2.0.0",
    "react-codemirror": "^1.0.0",
    "react-mathjax": "^0.1.1",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "sass-loader": "^7.0.3",
    "url-loader": "^1.0.1"
  }
}
